{
    "presets": ["env", "react"],
    "plugins": [
        "transform-object-rest-spread",
        [
            "transform-class-properties",
            {
                "spec": true
            }
        ],
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ]
}